{
  "executable": "APP",
  "splashscreen": {
    "imageFile": "/wwwroot/icone.png"
  },
  "singleInstance": false,
  "build": {
    "appId": "com.APP.app",
    "productName": "SicronizarBoard",
    "copyright": "Copyright Â© 2024",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "win": {
      "icon": "bin/Assets/icone.ico"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}